{
  "name": "@growing-web/toolkit",
  "version": "0.1.1",
  "description": "@growing-web/toolkit for Growing Web Guidelines.",
  "keywords": [
    "growing-web",
    "toolkit"
  ],
  "homepage": "https://github.com/growing-web/toolkit#readme",
  "bugs": {
    "url": "https://github.com/growing-web/toolkit/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/growing-web/toolkit.git"
  },
  "license": "MIT",
  "author": "vben",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    },
    "./cli": {
      "import": "./dist/cli.mjs",
      "require": "./dist/cli.cjs",
      "default": "./dist/cli.mjs",
      "types": "./dist/cli.d.ts"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "web-toolkit": "./bin/web-toolkit.mjs"
  },
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "prepublishOnly": "pnpm run build",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "husky install",
    "lint": "eslint . --fix",
    "test": "vitest"
  },
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown"
    ],
    "src/**/*.d.ts": [
      "eslint --ext .ts"
    ],
    "src/*/{src,types}/**/*.ts": [
      "eslint --ext .ts"
    ]
  },
  "dependencies": {
    "cac": "^6.7.12",
    "consola": "^2.15.3",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.1.0",
    "read-yaml-file": "^2.1.0",
    "strip-json-comments": "^4.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.3.0",
    "@commitlint/config-conventional": "^16.2.4",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-define-config": "^1.4.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "esno": "^0.15.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "picocolors": "^1.0.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.4",
    "unbuild": "^0.7.4",
    "vite": "^2.9.9",
    "vitest": "^0.12.6"
  }
}
